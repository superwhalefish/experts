<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shzp.sys.dao.AccountRoleDao">
<select id="findRoleByAcc_code" parameterType="String" resultType="String">
SELECT role_code FROM `sys_account_role` where acc_code =#{acc_code}
</select>

<delete id="delRole_codeByAcc_code" parameterType="String">
delete from `sys_account_role` where acc_code =#{acc_code}
</delete>
</mapper>